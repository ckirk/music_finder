<h1>Listing queries</h1>
<br>
 
<table>
  <tr>
    <th>Artist</th>
    <th>Track</th>
    <th>Duration</th>
  </tr>
 
  <% @queries.each do |query| %>
    <tr>
      <td><%= query.artist %></td>
      <td><%= query.track %></td>
      <td><%= query.duration %></td>
    </tr>
  <% end %>
</table>

<br>
<br>
<br>


<h1>Listing results</h1>
<br>
 
<table>
  <tr>
    <th>Title</th>
    <th>Duration</th>
    <th>View Count</th>
    <th>Like Count</th>
    <th>query_id</th>
    <th>Date Published</th>
    <th>Result Order</th>
    <th>category</th>
    <th>video_id</th>
    <th>Match?</th>
  </tr>
 
  <% @results.each do |result| %>
    <tr>
      <td><%= result.title %></td>
      <td><%= result.duration %></td>
      <td><%= result.view_count %></td>
      <td><%= result.like_count %></td>
      <td><%= result.query_id %></td>
      <td><%= result.date_published %></td>
      <td><%= result.order %></td>
      <td><%= result.category %></td>
      <td><%= result.video_id %></td>
      <td><%= result.match %></td>
    </tr>
  <% end %>
</table>