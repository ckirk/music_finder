<h1>Listing queries</h1>
<br>
 
<table>
  <tr>
    <th>Artist</th>
    <th>Track</th>
    <th>Duration</th>
  </tr>
 
  <% @queries.each do |query| %>
    <tr>
      <td><%= query.artist %></td>
      <td><%= query.track %></td>
      <td><%= query.duration %></td>
    </tr>
  <% end %>
</table>

<br>
<br>
<br>


<h1>Listing results</h1>
<br>
 
<table>
  <tr>
    <th>Title</th>
    <th>Duration</th>
    <th>View Count</th>
  </tr>
 
  <% @results.each do |result| %>
    <tr>
      <td><%= result.title %></td>
      <td><%= result.duration %></td>
      <td><%= result.view_count %></td>
    </tr>
  <% end %>
</table>